package JavaClasses;

public class Encapsulation {

	
	private int a = 12;
	
	private int b = 13 ;
	
	
	public int getA() {
		return a;
	}

	public void setA(int a) {
		this.a = a;
	}

	public int getB() {
		return b;
	}

	public void setB(int b) {
		this.b = b;
	}

	
	
	//setters and getters

	
	
}
