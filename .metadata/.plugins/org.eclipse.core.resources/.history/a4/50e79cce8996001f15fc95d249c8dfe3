package JavaClasses;

public class Methods {
	
	
	//Methods 
	
	1. Return Type - With Return Type , Without Return Type
	
	2. Parameter

	
	void add() 
	
	{
	
		System.out.println("Call my name");
	
}
	
    int add1() 
	
	{
	
		return 15;
	
}
	public static void main(String[] args) {
		
		//To perform set of actions 
		//we can avoid duplication of code
		//Declartion of methods
		//User defined methods
		
		Methods me = new Methods();
		
		int c =me.add1();
		
		System.out.println(c);
		
		
		

}
}
