package JavaClasses;

public class MethodswithParameters {
	
	
	int add(int a , int b)
	{
	
		int c = a + b;
		
		return c;
		
	}

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		
		MethodswithParameters m = new MethodswithParameters();
		
	int d 	= m.add(10,20);
	
	System.out.println(d);
		
		
		
		
		
		

	}

}
